HANDLE CreateProcessB(
  STARTUPINFO si,
  PROCESS_INFORMATION pi,
  TCHAR path)
{
  ZeroMemory(&si, sizeof(si));
  si.cb = sizzeof(si);
  ZeroMemory(&pi, sizeof(pi));
  
  if(!CreateProcess())
  {
    fprintf();
  }
  
}
